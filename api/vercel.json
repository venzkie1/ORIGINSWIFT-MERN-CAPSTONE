{
    "version": 2,
    "builds": [
      {
        "src": "./server.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "/server.js"
      }
    ],
    "build": {
      "env": {
        "MY_VAR": "my-value"
      },
      "maxDuration": 60
    }
}
  
  
  
  